<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="TunnelTimeout" value="500"/> <!-- seconds -->
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <handlers>
      <add name="HttpTunnelHandler" path="/tunnel" verb="*" type="Q.Net.Web.HttpTunnelHandler, HttpTunnelWeb" />
      <add name="DebugHandler" path="/debug" verb="*" type="Q.Net.Web.DebugHandler, HttpTunnelWeb" />
    </handlers>
    <modules>
      <!--<add name="HttpTunnelModule" type="Q.Net.Web.HttpTunnelModule, HttpTunnelWeb" />-->
    </modules>
  </system.webServer>
</configuration>
