<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <handlers>
      <add name="HttpTunnelHandler" path="/gateway" verb="*" type="Q.Net.Web.HttpTunnelHandler" />
    </handlers>
    <modules>
      <add name="HttpTunnelModule" type="Q.Net.Web.HttpTunnelModule" preCondition="integratedMode"/>
    </modules>
  </system.webServer>
</configuration>
